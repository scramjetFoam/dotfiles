        <key>Label</key>
        <string>com.vitorgalvao.launchd.wallpaperchange</string>
        <key>ProgramArguments</key>
        <array>
                <string>/usr/bin/osascript</string>
                <string>-l</string>
                <string>JavaScript</string>
                <string>-e</string>
                <string>const systemEvents = Application(&apos;System Events&apos;)
const wallpaperDir = systemEvents.currentDesktop.picturesFolder()
const wallpaperPaths = systemEvents.folders.byName(wallpaperDir).diskItems().map(p =&gt; p.toString())
const wallpaperRandom = wallpaperPaths[Math.floor(Math.random() * wallpaperPaths.length)]
systemEvents.currentDesktop.picture = wallpaperRandom</string>
        </array>
